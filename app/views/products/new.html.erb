<h1>New Product</h1>

<%= link_to 'Back', products_path %>

<%= form_for @product do |f| %>

	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.label :price_in_cents %>
	<%= f.number_field :price_in_cents %>

<%= f.submit %>
<% end %>
