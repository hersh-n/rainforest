<h1>Products#edit</h1>
<p>Find me in app/views/products/edit.html.erb</p>

<%= form_for @product do |f| %>

	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.label :price_in_cents %>
	<%= f.number_field :price_in_cents %>

<%= f.submit %>
<% end %>



